<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/launch_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/size_40"
        android:background="@drawable/launch_logo" />

    <TextView
        android:id="@+id/launch_iv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/launch_logo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/size_10"
        android:gravity="center"
        android:text="@string/launch_tv"
        android:textColor="@color/textview_color"
        android:textSize="@dimen/size_20sp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/launch_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:textColor="@color/textview_color"
        android:textSize="@dimen/size_10sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/size_16"
        android:drawableLeft="@drawable/launch_banquan_logo"
        android:drawablePadding="@dimen/size_5"
        android:text="@string/statement"
        android:textColor="@color/textview_color"
        android:textSize="@dimen/size_10sp" />

</RelativeLayout>